{
  "name": "synth-kit",
  "description": "Synthesizers construction kit",
  "version": "0.0.0",
  "main": "dist/synth-kit.js",
  "jsnext:main": "lib/index.js",
  "module": "lib/index.js",
  "license": "MIT",
  "repository": "oramics/synth-kit",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "format": "prettier-standard-formatter lib/* test/*",
    "test": "eslint lib && jest --coverage",
    "prepublish": "npm run test && npm run dist && npm run docs",
    "dist": "rollup -cm -n SynthKit -f umd -i lib/index.js -o dist/synth-kit.js && cp dist/* docs/dist/",
    "watch": "rollup --watch -cm -n SynthKit -f umd -i lib/index.js -o dist/synth-kit.js",
    "release": "npm run build && npm test && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish",
    "docs": "docker lib/ -o docs/literate",
    "docco": "docco lib/* lib/**/* -o docs/literate",
    "jsdoc": "jsdoc -c jsdoc3.config.json && open docs/api/index.html"
  },
  "authors": "@danigb",
  "keywords": [
    "audio",
    "synthetizer",
    "WebAudio",
    "music",
    "live-coding"
  ],
  "babel": {
    "presets": "es2015"
  },
  "devDependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "docco": "^0.7.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "jest": "^19.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "web-audio-test-api": "^0.5.2"
  },
  "dependencies": {
    "postman-jsdoc-theme": "0.0.2"
  }
}
